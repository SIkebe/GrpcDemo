  <ItemGroup>
    <Watch Include="..\GrpcDemo.Core\Protos\*.proto" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\GrpcDemo.Core\GrpcDemo.Core.csproj" />
  </ItemGroup>

</Project>
